#: ../terminal/main.c:42
msgid "Usage: Terminal [OPTION...]"
msgstr "Uso: Terminal [OPCIÓN...]"

#: ../terminal/main.c:44
msgid "  -h, --help                          Print this help message and exit"
msgstr "  -h, --help                          Mostrar esta ayuda y salir"

#: ../terminal/main.c:45
msgid ""
"  -v, --version                       Print version information and exit"
msgstr "  -v, --version                       Mostrar la versión y salir"

#: ../terminal/main.c:46
msgid ""
"  --disable-server                    Do not register with the D-BUS\n"
"                                      session message bus"
msgstr ""
"  --disable-server                    No registrarse en el bus de\n"
"                                      mensajes de sesión D_BUS"

#: ../terminal/main.c:49
msgid ""
"  -x, --execute                       Execute the remainder of the command\n"
"                                      line inside the terminal"
msgstr ""
"  -x, --execute                       Ejecutar lo que queda de la línea de "
"comandos\n"
"                                      en el terminal"

#: ../terminal/main.c:51
msgid ""
"  -e, --command=STRING                Execute the argument to this option\n"
"                                      inside the terminal"
msgstr ""
"  -e, --command=CADENA                Ejecutar el argumento de esta opción\n"
"                                      dentro de el terminal"

#: ../terminal/main.c:53
msgid ""
"  --working-directory=DIRNAME         Set the terminals working directory"
msgstr ""
"  --working-directory=DIR             Poner el valor del directorio de "
"trabajo"

#: ../terminal/main.c:54
msgid "  -T, --title=TITLE                   Set the terminals title"
msgstr "  -T, --title=TITLE                   Poner el título del terminal"

#: ../terminal/main.c:55
#, fuzzy
msgid ""
"  -H, --hold                          Do not immediately destroy the tab\n"
"                                      when the child command exits"
msgstr ""
"  -x, --execute                       Ejecutar lo que queda de la línea de "
"comandos\n"
"                                      en el terminal"

#: ../terminal/main.c:58
msgid ""
"  --display=DISPLAY                   X display to use for the last-\n"
"                                      specified window"
msgstr ""
"  --display=DISPLAY                   X display que usar para la última\n"
"                                      ventana especificada"

#: ../terminal/main.c:60
msgid ""
"  --geometry=GEOMETRY                 X geometry specification (see \"X\"\n"
"                                      man page), can be specified once per\n"
"                                      window to be opened"
msgstr ""
"  --geometry=GEOMETRY                 especificación de la geometría X (ver "
"la\n"
"                                      página del man de \"X\"), que puede "
"ser\n"
"                                      especificada una vez por cada ventana "
"a\n"
"                                      abrirse"

#: ../terminal/main.c:63
msgid ""
"  --role=ROLE                         Set the role for the last-specified;\n"
"                                      window; applies to only one window;\n"
"                                      can be specified once for each window\n"
"                                      you create from the command line"
msgstr ""
"  --role=REGLA                        Fijar la regla para la última ventana\n"
"                                      especificada; se aplica solamente a "
"una\n"
"                                      ventana; puede ser especificada una "
"vez\n"
"                                      por cada ventana que crea en la línea "
"de\tn                                      comandos"

#: ../terminal/main.c:67
msgid ""
"  --startup-id=STRING                 ID for the startup notification\n"
"                                      protocol"
msgstr ""
"  --startup-id=CADENA                 Identificador para el protocolo\n"
"                                      de notificación de arranque"

#: ../terminal/main.c:69
msgid ""
"  --fullscreen                        Set the last-specified window into\n"
"                                      fullscreen mode; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line."
msgstr ""
"  --fullscreen                        Colocar la última ventana "
"especificada\n"
"                                      a pantalla completa; se aplica sólo a "
"una\n"
"                                      ventana; puede especificarse una vez "
"por\n"
"                                      cada ventana que cree desde la línea "
"de\n"
"                                      comandos."

#: ../terminal/main.c:74
msgid ""
"  --show-menubar                      Turn on the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-menubar                      Colocar la barra de menú para la "
"última\n"
"                                      ventana especificada; se aplica a sólo "
"una\n"
"                                      ventana; puede ser especificada una "
"vez por cada\n"
"                                      ventana creada desde la línea de "
"comandos"

#: ../terminal/main.c:79
msgid ""
"  --hide-menubar                      Turn off the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-menubar                      Apagar la barra de menú para la "
"última\n"
"                                      ventana especificada; sólo aplicable a "
"una                                      ventana; puede ser especificada "
"para cada\n"
"                                      ventana que cree desde la línea de "
"comandos"

#: ../terminal/main.c:84
msgid ""
"  --show-borders                      Turn on the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --show-borders                      Mostrar las decoraciones de ventana "
"para\n"
"                                      la última ventana especificada; "
"aplicable sólo\n"
"                                      a una ventana; puede ser especificada "
"una vez\n"
"                                      para cada ventana que crea desde la "
"línea\n"
"                                      de comandos"

#: ../terminal/main.c:89
msgid ""
"  --hide-borders                      Turn off the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --hide-borders                      Apagar las decoraciones de la ventana "
"para\n"
"                                      las última ventana especificada; "
"aplicable\n"
"                                      a solamente una ventana; puede "
"especificarse una\n"
"                                      vez por cada ventana que cree desde la "
"línea de\n"
"                                      comandos"

#: ../terminal/main.c:94
msgid ""
"  --show-toolbars                     Turn on the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-toolbars                     Mostrar las barras de tareas para la "
"última ventana\n"
"                                      espeficada; aplicable a solamente una "
"ventana;\n"
"                                      puede especificarse una vez por cada "
"ventana que\n"
"                                      cree desde la línea de comandos"

#: ../terminal/main.c:99
msgid ""
"  --hide-toolbars                     Turn off the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-toolbars                     Ocultar las barras de tareas para la "
"última ventana\n"
"                                      espeficada; aplicable a solamente una "
"ventana;\n"
"                                      puede especificarse una vez por cada "
"ventana que\n"
"                                      cree desde la línea de comandos"

#: ../terminal/main.c:105
msgid ""
"  --tab                               Open a new tab in the last-specified\n"
"                                      window; more than one of these\n"
"                                      options can be provided"
msgstr ""
"  --tab                               Abrir una pestaña en la última ventana "
"especificada;\n"
"                                      Se puede usar más de una de estas "
"opciones"

#: ../terminal/main.c:108
msgid ""
"  --window                            Open a new window containing one tab;\n"
"                                      more than one of these options can be\n"
"                                      provided"
msgstr ""
"  --window                            Abrir una ventana conteniendo una "
"pestaña;\n"
"                                      Se puede proporcionar más de una de "
"estas\n"
"                                      opciones"

#: ../terminal/main.c:112
msgid "  --default-display=DISPLAY           default X display to use"
msgstr "  --default-display=DISPLAY           X display que usar por defecto"

#: ../terminal/main.c:113
msgid ""
"  --default-working-directory=DIRNAME Set the default terminals working\n"
"                                      directory"
msgstr ""
"  --default-working-directory=DIR     Fijar el directorio de trabajo por\n"
"                                      defecto del terminal"

