#: ../terminal/terminal-options.c:251
#, c-format
msgid ""
"Option \"--default-display\" requires specifying the default X display as "
"its parameter"
msgstr "Η επιλογή \"--default-display\" απαιτεί ως παράμετρο το προσδιορισμό προκαθορισμένης προβολής X"

#: ../terminal/terminal-options.c:267
#, c-format
msgid ""
"Option \"--default-working-directory\" requires specifying the default "
"working directory as its parameter"
msgstr "Η επιλογή \"--default-working-directory\" απαιτεί ως παράμετρο το προσδιορισμό προκαθορισμένου καταλόγου εργασίας"

#: ../terminal/terminal-options.c:284
#, c-format
msgid ""
"Option \"--execute/-x\" requires specifying the command to run on the rest "
"of the command line separated from \"--execute/-x\""
msgstr "Η επιλογή \"--execute/-x\" απαιτεί το προσδιορισμό εντολής που θα εκτελεστεί στη γραμμή εντολών μετά και χωριστά από το \"--execute/-x\""

#: ../terminal/terminal-options.c:302
#, c-format
msgid ""
"Option \"--command/-e\" requires specifying the command to run as its "
"parameter"
msgstr "Η επιλογή \"--command/-e\" απαιτεί ως παράμετρο το προσδιορισμό της εντολής που θα εκτελεστεί"

#: ../terminal/terminal-options.c:319
#, c-format
msgid ""
"Option \"--working-directory\" requires specifying the working directory as "
"its parameter"
msgstr "Η επιλογή \"--working-directory\" απαιτεί ως παράμετρο το προσδιορισμό καταλόγου εργασίας"

#: ../terminal/terminal-options.c:334
#, c-format
msgid "Option \"--title/-T\" requires specifying the title as its parameter"
msgstr "Η εντολή \"--title/-T\" απαιτεί ως παράμετρο το προσδιορισμό του τίτλου"

#: ../terminal/terminal-options.c:349
#, c-format
msgid ""
"Option \"--dynamic-title-mode\" requires specifying the dynamic title mode "
"as its parameter"
msgstr "Η επιλογή \"--dynamic-title-mode\" απαιτεί ως παράμετρο το προσδιορισμό λειτουργίας του δυναμικού τίτλου"

#: ../terminal/terminal-options.c:364
#, c-format
msgid "Invalid argument for option \"--dynamic-title-mode\": %s"
msgstr "Άκυρη παράμετρος για την επιλογή \"--dynamic-title-mode\": %s"

#: ../terminal/terminal-options.c:374
#, c-format
msgid ""
"Option \"--initial-title\" requires specifying the initial title as its "
"parameter"
msgstr "Η εντολή \"--initial-title\" απαιτεί ως παράμετρο το προσδιορισμό αρχικού τίτλου"

#: ../terminal/terminal-options.c:398 ../terminal/terminal-options.c:417
#, c-format
msgid "Option \"%s\" requires specifying the color as its parameter"
msgstr "Η επιλογή \"%s\" απαιτεί ως παράμετρο το προσδιορισμό χρώματος"

#: ../terminal/terminal-options.c:405 ../terminal/terminal-options.c:424
#, c-format
msgid "Unable to parse color: %s"
msgstr "Δεν είναι δυνατή η ανάλυση του χρώματος: %s"

#: ../terminal/terminal-options.c:435
#, c-format
msgid "Option \"--display\" requires specifying the X display as its parameter"
msgstr "Η επιλογή \"--display\" απαιτεί ως παράμετρο το προσδιορισμό προβολής X"

#: ../terminal/terminal-options.c:450
#, c-format
msgid ""
"Option \"--geometry\" requires specifying the window geometry as its "
"parameter"
msgstr "Η επιλογή \"--geometry\" απαιτεί ως παράμετρο το προσδιορισμό γεωμετρίας του παραθύρου"

#: ../terminal/terminal-options.c:465
#, c-format
msgid "Option \"--role\" requires specifying the window role as its parameter"
msgstr "Η επιλογή \"--role\" απαιτεί ως παράμετρο το προσδιορισμό ρόλου του παραθύρου"

#: ../terminal/terminal-options.c:480
#, c-format
msgid ""
"Option \"--workspace\" requires specifying the workspace number as its "
"parameter"
msgstr "Η επιλογή \"--workspace\" απαιτεί ως παράμετρο το προσδιορισμό αριθμού του χώρου εργασίας"

#: ../terminal/terminal-options.c:494
#, c-format
msgid ""
"Option \"--sm-client-id\" requires specifying the unique session id as its "
"parameter"
msgstr "Η επιλογή \"--sm-client-id\" απαιτεί ως παράμετρο το προσδιορισμό αναγνωριστικού της συνεδρίας"

#: ../terminal/terminal-options.c:509
#, c-format
msgid ""
"Option \"--startup-id\" requires specifying the startup id as its parameter"
msgstr "Η επιλογή \"--startup-id\" απαιτεί ως παράμετρο το προσδιορισμό αναγνωριστικού της εκκίνησης"

#: ../terminal/terminal-options.c:525
#, c-format
msgid ""
"Option \"--icon/-I\" requires specifying an icon name or filename as its "
"parameter"
msgstr "Η επιλογή \"--icon/-I\" απαιτεί ως παράμετρο το προσδιορισμό ονόματος του εικονιδίου ή του αρχείου"

#: ../terminal/terminal-options.c:607
#, c-format
msgid "Option \"--font\" requires specifying the font name as its parameter"
msgstr "Η επιλογή \"--font\" απαιτεί ως παράμετρο το προσδιορισμό ονόματος της γραμματοσειράς"

#: ../terminal/terminal-options.c:625
#, c-format
msgid "Option \"--zoom\" requires specifying the zoom (%d .. %d) as its parameter"
msgstr "Η επιλογή \"--zoom\" απαιτεί ως παράμετρο το προσδιορισμό μεγέθυνσης/σμίκρυνσης (%d .. %d)"

